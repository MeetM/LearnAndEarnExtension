{
  "name": "Learn And Earn",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "imgs/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Learn And Earn"
  },
  "content_scripts": [
    {
      "matches": ["https://www.khanacademy.org/*"],
      "js": ["js/jquery-3.5.1.min.js", "js/content.js"]
    }
  ],
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E",
    "cookies",
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite"
  ]
}
